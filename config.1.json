{
  "implement": "python",
  "dataset": {
    "x_path": "./iris_x.txt",
    "y_path": "./iris_y.txt"
  },
  "class": 3,
  "net":{
    "one_hot": true,
    "normal": true,
    "bone": [
      [2,6],
      "sigmoid",
      [6,10],
      "sigmoid",
      [10,6],
      "sigmoid",
      [6,3],
      "softmax"
    ],
    "cost_function": "cross_entropy",
    "optimizer": ["gradient_descent", 0.01]
  },
  "batch_size": 150
}